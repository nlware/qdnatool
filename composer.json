{
	"minimum-stability": "dev",
	"prefer-stable" : true,
	"config" : {
		"vendor-dir" : "app/Vendor/"
	},
	"repositories" : [
		{
			"type": "package",
			"package": {
				"name": "htmlawed/htmlawed",
				"version": "1.1.22",
				"type": "cakephp-vendor",
				"dist": {
					"url": "https://www.bioinformatics.org/phplabware/downloads/htmLawed1122.zip",
					"type": "zip"
				},
				"require": {
					"composer/installers": "*"
				}
			}
		}
	],
	"extra": {
		"installer-paths": {
			"app/Plugin/DebugKit": ["cakephp/debug_kit"],
			"app/Plugin/TwitterBootstrap": ["slywalker/twitter_bootstrap"]
		}
	},
	"require" : {
		"composer/installers": "~1.0",
		"cakephp/cakephp": "^2.0",
		"cakephp/debug_kit": "^2.2",
		"slywalker/twitter_bootstrap": "1.5.7",
		"htmlawed/htmlawed": "1.1.*",
		"simplesamlphp/simplesamlphp": "^1.0",
		"cturbelin/rserve-php": "2.0.x-dev"
	},
	"require-dev": {
		"phpunit/phpunit": "^3.7",
		"cakephp/cakephp-codesniffer": "^1.0"
	}
}
