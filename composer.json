{
	"minimum-stability": "dev",
	"prefer-stable" : true,
	"config" : {
		"secure-http": false,
		"vendor-dir" : "app/Vendor"
	},
	"repositories" : [
		{
			"type": "package",
			"package": {
				"name": "htmlawed/htmlawed",
				"version": "1.1.22",
				"type": "cakephp-vendor",
				"dist": {
					"url": "http://www.bioinformatics.org/phplabware/downloads/htmLawed1122.zip",
					"type": "zip"
				},
				"require": {
					"composer/installers": "*"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "nlware/rserve-php",
				"version": "dev-master",
				"type": "cakephp-vendor",
				"dist": {
					"type": "zip",
					"url": "https://github.com/nlware/rserve-php/archive/master.zip"
				},
				"source": {
					"type": "git",
					"url": "https://github.com/nlware/rserve-php",
					"reference": "master"
				},
				"require": {
					"composer/installers": "*"
				}
			}
		}
	],
	"extra": {
		"installer-paths": {
			"app/Plugin/DebugKit": ["cakephp/debug_kit"],
			"app/Plugin/TwitterBootstrap": ["slywalker/twitter_bootstrap"]
		}
	},
	"require" : {
		"composer/installers": "~1.0",
		"cakephp/cakephp": "2.9.*",
		"cakephp/debug_kit": "2.2.*",
		"slywalker/twitter_bootstrap": "1.5.7",
		"htmlawed/htmlawed": "1.1.*",
		"nlware/rserve-php": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	}
}
